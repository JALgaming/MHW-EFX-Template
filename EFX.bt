// type 0 - float; 1 - int; 2 - ubyte; 3 - jitterless float
typedef struct (int type, int skip){
    if(type==0 || type == 3){
        if (skip==1){
            float fixed_x;
            float random_x;
            //FSkip(skip*4);
            float fixed_y;
            float random_y;
            //FSkip(skip*4);
            float fixed_z;
            float random_z;
            //FSkip(skip*4);
        }else{
            float x;
            FSkip(skip*4);
            float y;
            FSkip(skip*4);
            float z;
            FSkip(skip*4);
        }
//TODO - Fix skip != 1
    } else if (type == 2){
        ubyte x;
        ubyte y;
        ubyte z;
        ubyte NULL<bgcolor=0xffffff>;
    } else {
        int x;
        int y;
        int z;
    }
} XYZ;

